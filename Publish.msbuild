<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Publish" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<SourceLocation />
		<TargetLocation />
	</PropertyGroup>
	
	<Target Name="Publish">
		<Exec Command="robocopy $(SourceLocation) $(TargetLocation)" ContinueOnError="true" />
	</Target>
</Project>